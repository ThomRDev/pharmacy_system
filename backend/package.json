{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --no-cache --detectOpenHandles --forceExit --passWithNoTests",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --no-cache --detectOpenHandles",
    "test:coverage": "jest --coverage --no-cache --detectOpenHandles",
    "clean": "rimraf dist/ && rimraf src/migrations",
    "start": "npm run clean && tsc --project tsconfig.node.json && node ./dist/index.js",
    "start:dev": "npm run clean && tsc --project tsconfig.node.json && concurrently \"tsc -w\" \"cross-env NODE_ENV=test nodemon ./dist/index.js\"",
    "start:prod": "SET NODE_ENV=production && npm start",
    "typeorm": "typeorm-ts-node-esm -d ./src/config/data.source.ts",
    "m:gen": "npm run typeorm migration:generate",
    "m:run": "npm run typeorm migration:run",
    "m:gen:test": "cross-env NODE_ENV=test npm run typeorm migration:generate",
    "m:run:test": "cross-env NODE_ENV=test npm run typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "license": "ISC"
}
